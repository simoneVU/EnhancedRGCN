{
  "name": "@triply/utils",
  "version": "1.3.79",
  "description": "",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "license": "UNLICENSED",
  "author": "Triply <info@triply.cc>",
  "scripts": {
    "build": "tsc -b ./tsconfig-build.json",
    "build:watch": "tsc -b ./tsconfig-build.json --watch --preserveWatchOutput",
    "clean": "rm -rf ./lib && rm ./tsconfig-build.tsbuildinfo",
    "util:prettify": "prettier --parser typescript --write $(find ./src -name '*.ts')",
    "test": "mocha $(find ./lib -name '*-test.js') --require source-map-support/register",
    "preversion": "node ./lib/bin/isCleanBranch.js",
    "postversion": "node ./lib/bin/versionToBranch.js utils",
    "prepublishOnly": "chmod +x ./lib/bin/*.js",
    "util:validateTs": "tsc -p ./tsconfig-validate.json",
    "util:validateTs:watch": "tsc -p ./tsconfig-validate.json --watch --preserveWatchOutput",
    "util:lint": "ESLINT_STRICT=true ESLINT_EXPENSIVE=true eslint --ext .tsx --ext .ts ./src",
    "util:validatePrettier": "prettier --parser typescript --check $(find ./src -regex '.*\\.tsx?$')",
    "util:validateFull": "yarn run util:validateTs && yarn run util:lint && yarn run build && yarn run test"
  },
  "bin": {
    "versionToBranch": "./lib/bin/versionToBranch.js",
    "isCleanBranch": "./lib/bin/isCleanBranch.js",
    "calverIncrement": "./lib/bin/calverIncrement.js",
    "ensureCalver": "./lib/bin/ensureCalver.js"
  },
  "devDependencies": {
    "@types/chai-as-promised": "^7.1.3",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^8.3.0",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@types/n3": "^1.4.4",
    "@types/sparqljs": "^3.1.1",
    "autocast": "0.0.4",
    "deepdash": "^5.2.1",
    "fs-extra": "^9.0.1",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.19",
    "n3": "^1.8.0",
    "rdf-string": "^1.4.2",
    "source-map-support": "^0.5.19",
    "sparqljs": "3.4.1",
    "through2": "^4.0.2",
    "ts-essentials": "^7.0.1",
    "url-parse": "^1.4.4",
    "wellknown": "^0.5.0"
  },
  "resolutions": {}
}
